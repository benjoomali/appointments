<p id="notice"><%= notice %></p>

<h1>Availability</h1>

<table>
  <thead>
    <tr>
      <th>start_time</th>
      <th>end_time</th>
      <th>calendar_id</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @availabilities.each do |availability| %>
      <tr>
        <td><%= availability.start_time %></td>
        <td><%= availability.end_time %></td>
        <td><%= availability.calendar_id %></td>
        <td><%= link_to 'Show', availability %></td>
        <td><%#= link_to 'Edit', edit_availability_path(availability) %></td>
        <td><%= link_to 'Delete', availability, method: :delete %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Availability', new_availability_path %>
